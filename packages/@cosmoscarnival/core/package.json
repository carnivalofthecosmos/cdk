{
  "name": "@cosmoscarnival/core",
  "version": "0.0.0",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/*.js",
    "lib/*.d.ts",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "clean": "rm lib/*.js lib/*.d.ts",
    "format": "prettier --write lib/**/*.ts bin/**/*.ts",
    "lint": "./node_modules/.bin/eslint lib/**/*.ts bin/**/*.ts",
    "tslint": "tslint -p tsconfig.json",
    "prepublish": "npm run tslint && npm run test && npm run build"
  },
  "dependencies": {
    "@aws-cdk/aws-ec2": "^1.23.0",
    "@aws-cdk/aws-ecr": "^1.23.0",
    "@aws-cdk/aws-ecs": "^1.23.0",
    "@aws-cdk/aws-elasticloadbalancingv2": "^1.23.0",
    "@aws-cdk/aws-elasticloadbalancingv2-targets": "^1.23.0",
    "@aws-cdk/aws-route53": "^1.23.0",
    "@aws-cdk/core": "^1.23.0"
  }
}
